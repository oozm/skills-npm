{
  "name": "skills-npm",
  "type": "module",
  "version": "0.0.3",
  "packageManager": "pnpm@10.28.2",
  "description": "CLI to install agents skills that shipped with your installed npm packages",
  "author": "Anthony Fu <anthonyfu117@hotmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/antfu",
  "homepage": "https://github.com/antfu/skills-npm#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antfu/skills-npm.git"
  },
  "bugs": "https://github.com/antfu/skills-npm/issues",
  "keywords": [
    "agent",
    "skills",
    "ai",
    "cli",
    "cursor",
    "claude"
  ],
  "sideEffects": false,
  "exports": {
    ".": "./dist/index.mjs",
    "./cli": "./dist/cli.mjs",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "bin": {
    "skills-npm": "./dist/cli.mjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "lint": "eslint",
    "prepublishOnly": "nr build",
    "release": "bumpp",
    "start": "tsx src/cli.ts",
    "test": "vitest",
    "typecheck": "tsc",
    "prepare": "git submodule update --init --recursive && simple-git-hooks && pnpm start"
  },
  "dependencies": {
    "@clack/prompts": "catalog:prod",
    "cac": "catalog:prod",
    "gray-matter": "catalog:prod",
    "picocolors": "catalog:prod",
    "tinyglobby": "catalog:prod",
    "unconfig": "catalog:prod",
    "xdg-basedir": "catalog:prod",
    "yaml": "catalog:prod"
  },
  "devDependencies": {
    "@antfu/eslint-config": "catalog:dev",
    "@antfu/ni": "catalog:dev",
    "@antfu/utils": "catalog:inlined",
    "@slidev/cli": "catalog:examples",
    "@types/node": "catalog:types",
    "bumpp": "catalog:dev",
    "eslint": "catalog:dev",
    "lint-staged": "catalog:dev",
    "publint": "catalog:dev",
    "simple-git": "catalog:dev",
    "simple-git-hooks": "catalog:dev",
    "tinyexec": "catalog:testing",
    "tsdown": "catalog:dev",
    "tsx": "catalog:dev",
    "typescript": "catalog:dev",
    "vite": "catalog:dev",
    "vitest": "catalog:testing",
    "vitest-package-exports": "catalog:testing"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
